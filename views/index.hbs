    <div class=".container">
        <div class="form">
            <h1 class="text">Realtor App</h1>
            <div class="form-container">
                <input #email type="email" name="email" placeholder="Enter your email">
                <br />
                <input #password type="password" name="password" placeholder="Enter your password">
                <br />
                <button type="button" role="button" (click)="login($event,email, password)">login</button>
                <form class="form-c" style="margin-top: 10px;" action="/auth/register" method="POST" >
                    <button type="submit"> Register Now</button>
                </form>
            </div>
        </div>
    </div>
